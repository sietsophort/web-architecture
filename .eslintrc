{
  "rules": {
    "camelcase": 0,
    "linebreak-style": 0,
    "consistent-return": 0,
    "array-callback-return": 0,
    "no-return-assign": 0,
    "max-len": 0,
    "no-undef": 0,
    "no-param-reassign": 0,
    "object-curly-newline": [
      "error",
      {
        "consistent": true
      }
    ],
    "no-underscore-dangle": 0,
    "prefer-destructuring": 0,
    "comma-dangle": [
      2,
      {
        "arrays": "always-multiline",
        "objects": "always-multiline",
        "imports": "always-multiline",
        "exports": "always-multiline",
        "functions": "ignore"
      }
    ]
  },
  "extends": ["airbnb"]
}
